{
    "redirects": [
        {
            "source": "/blog",
            "destination": "blog.lucasoliveira.io",
            "statusCode": 301
        }
    ],
    "rewrites": [
        {
            "source": "/my-stuff",
            "destination": "https://personal-stuff.vercel.app/"
        },
        {
            "source": "/my-stuff/:path*",
            "destination": "https://personal-stuff.vercel.app/:path*"
        }
    ]
}